#include <iostream>
#include <cmath>
#include <stdio.h>
#include <string>

int romeToArab(char rome){
    if (rome == 'I'){
        return 1;
    }
    if (rome == 'V'){
        return 5;
    }
    if (rome == 'X'){
        return 10;
    }
    if (rome == 'L'){
        return 50;
    }
    if (rome == 'C'){
        return 100;
    }
    if (rome == 'D'){
        return 500;
    }
    if (rome == 'M'){
        return 1000;
    }
    return  0;
}

